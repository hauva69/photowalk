[defaults]

import_directory=/backup/hauva/Pictures/incoming

[handlers]

keys=filehandler

[formatters]

keys=form01

[logger_root]
level=DEBUG
qualname=(root)
handlers=filehandler

[loggers]

keys=root

[handler_filehandler]

class=handlers.RotatingFileHandler
level=DEBUG
formatter=form01
args=('/home/hauva/logs/photowalk.log', 'a', 1048576, 10)

[handler_mailhandler]

class=handlers.SMTPHandler
host=smtp.FIXME.example.com
port=25
from=hauva69@gmai.com
to=hauva69@gmail.com
subject=sponsblog error
level=WARN
args=('smtp.FIXME.example.com', 'hauva69@gmail.com', ['hauva69@gmai.com'], 'Photowalk Error')

[formatter_form01]

format=F1 %(asctime)s %(levelname)s %(message)s
datefmt=
class=logging.Formatter
